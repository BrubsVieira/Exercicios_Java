package exer2;

public class ContaInvestimento extends ContaBancaria {

	
	@Override
	public double transferir(double valor) {	
		ContaBancaria.saldo = saldo - valor;
		return ContaInvestimento.saldo - ContaBancaria.saldo;
		
	}
	
	@Override
	public double calcularSaldo() {
		return (ContaInvestimento.saldo* 0.05)+ContaInvestimento.saldo; // mesma coisa de ((10/100)*saldo)-saldo;
	}

	

}
