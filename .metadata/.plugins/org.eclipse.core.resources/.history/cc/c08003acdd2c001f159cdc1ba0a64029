package exer2;

public class ContaCorrente extends ContaBancaria {

	@Override
	public double transferir(double valor) {
		double novo = ContaBancaria.saldo = saldo - valor;
		return ContaInvestimento.saldo + novo;
		
	}

	@Override
	public double calcularSaldo() {
		return  ContaCorrente.saldo-(ContaCorrente.saldo*0.10);
		
	}



}
