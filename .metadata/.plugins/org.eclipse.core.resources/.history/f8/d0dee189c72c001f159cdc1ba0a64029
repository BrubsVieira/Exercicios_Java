package exer2;

public class Aplicacao {

	public static void main(String[] args) {
		
		ContaBancaria corrente = new ContaCorrente(300);
		corrente.depositar(10);
		corrente.sacar(5);
		corrente.transferir(10,"Emmanuel Tito = Agencia 123");
		double saldo = corrente.calcularSaldo();
		System.out.println(saldo);
		
		
	}

}
