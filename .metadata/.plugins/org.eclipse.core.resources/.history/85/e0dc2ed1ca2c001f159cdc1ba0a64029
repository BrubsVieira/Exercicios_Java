package exer2;

public class Aplicacao {

	public static void main(String[] args) {
		
		ContaCorrente corrente = new ContaCorrente(300);
		corrente.depositar(10);
		corrente.sacar(0);
		corrente.transferir(0,"Emmanuel Tito = Agencia 123");
		double saldo = corrente.calcularSaldo();
		System.out.println(saldo);
		
	}

}
